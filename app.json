{
  "expo": {
    "name": "solana-cnft",
    "slug": "solana-cnft",
    "scheme": "solanacnft",
    "owner": "openai",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/YOUR_ACCOUNT/solana-cnft",
    "version": "0.1.1",
    "orientation": "portrait",
    "primaryColor": "#111827",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skeet.solanacnft"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "package": "com.skeet.solanacnft",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}